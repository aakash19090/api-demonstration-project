{
  "name": "react-frontend",
  "version": "1.0.0",
  "description": "This is React boiler plate for frontend JS projects which we you can clone and you will get all the right setup and helpful library and right framework",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config ./webpack.dev.js --mode development --open",
    "build": "webpack --config ./webpack.prod.js --mode production",
    "lint": "eslint src",
    "format": "prettier --write \"**/*.+(js|jsx|json|css)\"",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "author": "yamin lawar",
  "license": "ISC",
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "axios": "^0.21.1",
    "clear": "^0.1.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "rc-progress": "^3.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fileupload-progress": "^0.5.0",
    "react-hook-form": "^7.3.4",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-slidedown": "^2.4.5",
    "react-table": "^7.7.0",
    "react-toastify": "^7.0.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "autoprefixer": "^10.2.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^6.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-merge": "^5.7.3"
